      bp1.blogger.com&quot;;
      var imagenesblogger2 = &quot;
      bp2.blogger.com&quot;;
      var imagenesblogger3 = &quot;
      bp3.blogger.com&quot;;
      var blogger1 = &quot;
      www.blogger.com&quot;;
      var blogger2 = &quot;
      www2.blogger.com&quot;;
